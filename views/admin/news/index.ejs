<!-- Include head -->
<% include ../head %>
<!-- breadcrumb -->
<ul class="breadcrumb" style="background:#e2e2e2; color:#333;margin-left:250px;margin-top:10px;">
    <li><a href="/admin">Home</a></li>
    <li class="active">About us Management</li>
    <li class="active">List</li>
</ul>
<!-- Liste des utilisateurs -->
<div class="container">
  <div class="row">
    <div class="col-sm-12 card">
       <div class="row">
         <% if(notification!=undefined){%>
            <div class="alert alert-danger alert-dismissable">
                <button aria-hidden="true" data-dismiss="alert" class="close" type="button">Ã—</button>
                  <%= notification %>
            </div>
        <%}; %>
         <div class="col-sm-3 text-center">
           <h5>News List</h5>
         </div>
         <div class="col-sm-7">
           <form action="" method="post">
             <input type="text" class="form-control" name="search_input" placeholder="Find Info by title" style="font-size:10px;">
           </form>
         </div>
         <div class="col-sm-2" style="margin-top:4px;">
           <a href="/admin/about/create" class="btn btn-default" style="font-size:10px;"> <i class="fa fa-plus"></i> Add Info</a>
         </div>
       </div>
    </div>
  </div>
</div>
<div class="bg-color-sky-light">
    <div class="content-md container">
        <div class="row" id="utilisateurs">
          <% data.forEach(function(info) { %>
               <div class="col-md-3 md-margin-b-30">
                   <!-- Pricing List v4 -->
                   <div class="pricing-list-v4 radius-10">
                       <div class="pricing-list-v4-header">
                           <h4 class="pricing-list-v4-title"><i class="fa fa-tasks" style="font-size:80px;"></i></h4>
                           <span class="pricing-list-v4-subtitle" style="font-size:10px;"><%= info.titre %> </span>
                           <span class="pricing-list-v4-subtitle" style="font-size:10px;"><a href="/admin/about/edit/<%= info._id %>"><i class="fa fa-pencil"></i></a> <a href="/admin/about/delete/<%= info._id %>"><i class="fa fa-trash"></i></a> </span>
                       </div>
                           <div class="center-block" style="padding-botton:20px;">
                               <i class="fa fa-folder"></i>
                           </div>
                       </div>
                   </div>
          <% }); %>

                <!-- End Pricing List v4 -->
            </div>
        </div>
        <!--// end row -->
    </div>

<!-- End Pricing List v4 -->

<!-- Copyright -->
<div class="content-sm container text-center">
    <p>Tous Droits reserves ... <a href="">Natco</a></p>
</div>
<!-- End Copyright -->
<!--========== END PAGE CONTENT ==========-->

</body>
</html>
